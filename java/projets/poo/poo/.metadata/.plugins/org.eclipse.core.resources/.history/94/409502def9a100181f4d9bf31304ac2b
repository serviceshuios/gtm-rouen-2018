package fr.gtm.feu;

public abstract class Vehicule {
	private String marque;
	
	// Constucteur
	public Vehicule(String marque) {
		super();
		this.marque = marque;
	}
	
	
	public abstract void demarrer();


	public String getMarque() {
		return marque;
	}

	public void setMarque(String marque) {
		this.marque = marque;
	}
	
}
